reverse_word <- function() {
word <- readline(prompt = "Enter a word: ")
reversed <- ""
i <- nchar(word)
while (i > 0) {
reversed <- paste(reversed, substr(word, i, i), sep = "")
i <- i - 1
}
cat("Reversed word:", reversed, "\n")
}
reverse_word()
